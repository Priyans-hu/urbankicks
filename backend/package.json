{
  "name": "urbankicks-backend",
  "version": "1.0.0",
  "description": "Backend API for UrbanKicks e-commerce platform",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "start:legacy": "node --watch index.js",
    "dev": "ts-node src/app.ts",
    "dev:watch": "ts-node-dev --respawn src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
